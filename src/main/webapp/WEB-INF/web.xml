<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
		 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
		 version="4.0"
		 metadata-complete="true">
	<display-name>A Simple Web Application</display-name>

	<servlet>
		<servlet-name>existLogin</servlet-name>
		<servlet-class>org.xcompany.xprojects.servlets.ExistLoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>existLogin</servlet-name>
		<url-pattern></url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>regLogin</servlet-name>
		<servlet-class>org.xcompany.xprojects.servlets.RegLoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>regLogin</servlet-name>
		<url-pattern>/regLogin</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>userTurnsList</servlet-name>
		<servlet-class>org.xcompany.xprojects.servlets.UserTurnsListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>userTurnsList</servlet-name>
		<url-pattern>/userTurnsList</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>turnStandings</servlet-name>
		<servlet-class>org.xcompany.xprojects.servlets.TurnStandingsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>turnStandings</servlet-name>
		<url-pattern>/userTurnsList/turnStandings</url-pattern>
	</servlet-mapping>




	<servlet>
		<servlet-name>add</servlet-name>
		<servlet-class>org.xcompany.xprojects.servlets.AddServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>add</servlet-name>
		<url-pattern>/add</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>table</servlet-name>
		<servlet-class>org.xcompany.xprojects.servlets.TableServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>table</servlet-name>
		<url-pattern>/table</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>fileupload</servlet-name>
		<servlet-class>org.xcompany.xprojects.servlets.FileUploadDemo</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>fileupload</servlet-name>
		<url-pattern>/fileupload</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>turnTable</servlet-name>
		<servlet-class>org.xcompany.xprojects.servlets.TurnTableServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>turnTable</servlet-name>
		<url-pattern>/turntable</url-pattern>
	</servlet-mapping>

</web-app>